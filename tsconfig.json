{
   "extends": "./.svelte-kit/tsconfig.json",
    "compilerOptions": {
      "target": "ES2020",
      "module": "ESNext",
      "moduleResolution": "Node",
      "strict": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "allowJs": true,
      "checkJs": false,
      "noImplicitAny": true,
      "types": ["svelte"],
      "paths": {
        "$lib": ["src/lib"],
        "$types": [".svelte-kit/types"]
      }
    },
    "include": ["src/**/*", ".svelte-kit/types/**/*.d.ts"],
    "exclude": ["node_modules", "dist"]
  }